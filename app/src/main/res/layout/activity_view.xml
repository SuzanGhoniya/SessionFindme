<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background"
    android:textAlignment="center"
    android:padding="10dp"
    android:layout_marginBottom="50dp"
    android:id="@+id/colorChange"
    >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/maven_pro_medium"
            android:text="City:"
            android:textSize="10sp"
            android:layout_marginTop="5dp"
            android:layout_marginHorizontal="25dp"
            />

    <TextView
        android:id="@+id/cityName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/maven_pro_medium"
        android:text="city_name"
        android:textSize="20sp" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="center:"
            android:textSize="10sp"
            android:layout_marginVertical="5dp"
            android:layout_marginHorizontal="25dp"
            android:fontFamily="@font/maven_pro_medium"
            />
        <TextView
            android:id="@+id/centerName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="center name"
            android:layout_marginStart="50dp"
            android:textSize="20sp"
            android:fontFamily="@font/maven_pro_medium"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Address:"
            android:textSize="10sp"
            android:layout_marginVertical="5dp"
            android:layout_marginHorizontal="25dp"
            android:fontFamily="@font/maven_pro_medium"
            />
        <TextView
            android:id="@+id/centerAdd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="center Add"
            android:layout_marginStart="50dp"
            android:textSize="20sp"
            android:fontFamily="@font/maven_pro_medium"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Timing:"
            android:textSize="10sp"
            android:layout_marginVertical="5dp"
            android:layout_marginHorizontal="25dp"
            android:fontFamily="@font/maven_pro_medium"
            />
        <TextView
            android:id="@+id/vaccineTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="timing "
            android:layout_marginStart="50dp"
            android:textSize="20sp"
            android:fontFamily="@font/maven_pro_medium"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="vaccine Name:"
            android:textSize="10sp"
            android:layout_marginVertical="5dp"
            android:layout_marginHorizontal="25dp"
            android:fontFamily="@font/maven_pro_medium"
            />
        <TextView
            android:id="@+id/vaccineName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Vaccine Name"
            android:layout_marginStart="50dp"
            android:textSize="20sp"
            android:fontFamily="@font/maven_pro_medium"
            />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginHorizontal="25dp"
        android:layout_marginVertical="5dp"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="age limit"
                android:textSize="10sp"
                android:fontFamily="@font/maven_pro_medium"
                />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/age"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="45+"
                    android:textSize="20sp"
                    android:fontFamily="@font/maven_pro_medium"

                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="+"
                    android:textSize="20sp"
                    android:layout_marginEnd="30dp"
                    android:fontFamily="@font/maven_pro_medium"
                    />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginEnd="30dp"

            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Fee type:"
                android:textSize="10sp"
                android:fontFamily="@font/maven_pro_medium"
                />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/feeType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="fee "
                    android:textSize="19sp"
                    android:fontFamily="@font/maven_pro_medium"

                    />
            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Available Dose"
                android:fontFamily="@font/maven_pro_medium"
                android:textSize="10sp"
                />
            <TextView
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"

                android:textSize="19sp"
                android:fontFamily="@font/maven_pro_medium"

                />
        </LinearLayout>

    </LinearLayout>


</LinearLayout>